{
  "name": "zhiyicx/plus-install-plugin",
  "type": "composer-plugin",
  "description": "ThinkSNS Plus component require plugin.",
  "keywords": ["ts", "ts+", "plus", "thinksns"],
  "license": "MIT",
  "authors": [
    {
      "name": "Seven Du",
      "email": "shiweidu@outlook.com",
      "homepage": "http://medz.cn",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "composer-plugin-api": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5 || ^5.0.5",
    "phpunit/phpunit-mock-objects": "^2.3 || ^3.0",
    "composer/composer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Zhiyi\\Component\\Installer\\PlusInstallPlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zhiyi\\Component\\Installer\\PlusInstallPlugin\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "class": "Zhiyi\\Component\\Installer\\PlusInstallPlugin\\InstallerPlugin"
  }
}
